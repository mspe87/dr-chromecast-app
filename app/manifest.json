{
  "name": "DR-Chromecast",
  "version": "1",
  "manifest_version": 2,
  "description": "Chromecast support for dr.dk with DOM manipulation.",
  "web_accessible_resources": ["js/cast.js", "img/1.png"],
  "browser_action": {
    "name": "DR Chromecast",
    "icons": ["DR_large.png"],
    "default_icon": "DR_large.png"
  },
  "content_scripts": [ {
    "js": [ "jquery-2.1.3.min.js", "background.js" ],
    "matches": [ "http://*.dr.dk/tv/*"]
  }]
}